<%- include("partials/header.ejs") %>

<div class="post-content">
    <% if (locals.data) { %>
    <div class="content">
        <h1 class="content-title"><%= data[id].title %></h1>
        <p class="date"><%=data[id].date%></p>
        <P class="paragraph"><%=data[id].content%></P>
    </div>
    <% } else{%>
    <h1>Not found</h1>
    <% } %>
</div>

